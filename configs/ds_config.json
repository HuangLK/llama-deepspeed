{
    "train_micro_batch_size_per_gpu": 2,
    "train_batch_size": 64,
    "steps_per_print": 1000,
    "gradient_clipping": 1.0,
    "lr-decay-style": "cosine",
    "warmup": 0.03,
    "optimizer": {
        "type": "Adam",
        "params": {
            "lr": 1e-5,
            "betas": [0.9, 0.95],
            "eps": 1.0e-8
        }
    },
    "min_lr": 1e-6,

    "wall_clock_breakdown": true
}
